name: SpotifyChurnMLProject

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      n_estimators: {type: int, default: 100}
      max_depth: {type: int, default: 10}
      dataset_path: {type: str, default: "spotify_churn_dataset_preprocessing"}
    command: "python modelling.py --n_estimators {n_estimators} --max_depth {max_depth} --dataset_path {dataset_path}"